# MindMirror AI Frontend Environment Variables - Gradio Backend
# Copy this file to .env and fill in your actual values

# Backend API URL (Gradio server)
# Local development:
REACT_APP_API_URL=http://localhost:7860
# Production (update after deploying to HF Spaces):
# REACT_APP_API_URL=https://YOUR_USERNAME-mindmirror-ai.hf.space

# Google OAuth Client ID (from Google Cloud Console)
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
